rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    match /presentations/{document} {
      allow read;
    }
    
    match /awards/{document} {
      allow read;
    }
    
    match /publications/{document} {
      allow read;
    }
    
    match /media-commentary/{document} {
      allow read;
    }
    
    match /mentoring/{document} {
      allow read;
    }
  
  }
}